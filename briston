#define MAX_ROADBLOCKS 35

enum rInfo
{
	sMod,
    sCreated,
    Float:sXx,
    Float:sYy,
    Float:sZz,
    sObject,
};
new Roadblocks[MAX_ROADBLOCKS][rInfo];
enum hqLocks
{
	triadslock,
	bratvalock,
	ballaslock,
	sevillelock,
	rifalock,
	crllock,
	hitlock,
};
new hqlock[hqLocks];
