 	if(PlayerInfo[playerid][pAdmin] < 6 && GasesteReclama(message) || FindIP(message))
	{
	    format(strings, sizeof(strings), "%s[%d] a facut reclama pe h: %s",PlayerName(playerid),playerid, message);
