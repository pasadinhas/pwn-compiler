	if(gLogged[playerid] != 1) return SendClientMessage(playerid,COLOR_RED,"Nu esti logat");
	if(PlayerInfo[playerid][pAdmin] < 5) return SendClientMessage(playerid, COLOR_RED, "Nu ai rangul administrativ necesar!");
	new x;
