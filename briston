 	                    SetDynamicObjectMaterialText(graffity[playerid],0,"Seville",OBJECT_MATERIAL_SIZE_256x256,"Arial", 35, 1, 0xFFFFFFC8,0, 1);
					}
			  	}
                else return SendClientMessage(playerid,-1,"Nu ai Spray.");
			}
	}
