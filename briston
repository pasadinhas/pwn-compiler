    if(gLogged[playerid] != 1) return SendClientMessage(playerid,COLOR_RED,"Nu esti logat");
	new message[128];
	if(PlayerInfo[playerid][pHelper] < 1) return SendClientMessage(playerid, COLOR_RED, "Nu ai acces la aceasta comanda");
