					if(IsPlayerInRangeOfPoint(playerid,3,ox,oy,oz) && ox > 1)
					{
						if(PizzaOfPlayer[playerid]>=5) return GameTextForPlayer(playerid,"~r~~n~~n~~n~~n~~n~~n~~n~Aveti deja 5 cutii!",1500,3);
