	        new rand = random(100);
	        if(rand <= 10 || PlayerInfo[playerid][pAdmin] > 0)
	        {
	            GetPlayer2DRadarZone(playerid, zone, sizeof(zone));
	            new hasprotection = 0;
		    	if(HouseInfo[h][hProtection3] != 0) hasprotection = 1;
				if(hasprotection != 0)
				{
				    new Float:health;
				    GetPlayerHealth(playerid, health);
				    SetPlayerHealth(playerid, health-50);
