	GetDynamicObjectPos(object[playerid], simx, simy, simz);
	DestroyDynamicObject(object[playerid]);
	object[playerid] = CreateDynamicObject(354, simx, simy, simz+2, 0, 0, 0);
	SetTimerEx("destroyFlare",15000,0,"d",playerid);
}

