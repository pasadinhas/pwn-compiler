	if(strcmp(choice, "2", true) == 0)
	{
		ApplyAnimation(playerid,"BEACH", "SitnWait_loop_W", 4.0, 1, 0, 0, 0, 0); // Lay down
		SetPVarInt(playerid, "UsingAnim", 1);
	}
	return 1;
}
