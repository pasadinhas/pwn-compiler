     			{

					TogglePlayerControllable(playerid, 1);
					TextDrawHideForPlayer(playerid,PDMainMenu);
					TextDrawHideForPlayer(playerid,PDLineUp);
					TextDrawHideForPlayer(playerid,PDLineDown);
					TextDrawHideForPlayer(playerid,PDSsapd);
