		    SetPlayerInterior(playerid,0);
		}

		//hair
		else if (IsPlayerInRangeOfPoint(playerid, 2, 2071.7239,-1793.9116,13.5469))
		{
		   	SetPlayerPos(playerid, 412.1062, -54.1122, 1002.2984);
		   	SetPlayerInterior(playerid,12);
