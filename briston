		SendClientMessage(playerid,0x00FF00C8,"*FELICITARI, ai luat carnetul!");
		return 1;
	}
	//swep
	if(CP[playerid] == 1117)
    {
		SetPlayerCheckpoint(playerid, 1666.0861, -1870.6160, 13.5246, 2);
        CP[playerid] = 69000;
        format(string,sizeof(string), "%s %d [CP] 1",PlayerName(playerid),playerid);
		JobLog(string);
        return 1;
	}
