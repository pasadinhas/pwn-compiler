    if(newkeys == KEY_YES)
 	{
 	    if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER)
		{
		    if(VehicleLights[GetPlayerVehicleID(playerid)] == 0)
			{
				VehicleLights[GetPlayerVehicleID(playerid)] = 1;
