	if(PlayerInfo[playerid][pLeader] == 4 || PlayerInfo[playerid][pLeader] == 4 || PlayerInfo[playerid][pMember] == 12 || PlayerInfo[playerid][pMember] == 12 || PlayerInfo[playerid][pLeader] == 7 || PlayerInfo[playerid][pMember] == 7)
