	vInfo[Total_Veh_Created][vTowed],
	vInfo[Total_Veh_Created][trWeapon1],
	vInfo[Total_Veh_Created][trWeapon2],
	vInfo[Total_Veh_Created][trWeapon3],
	vInfo[Total_Veh_Created][trWeapon4],
