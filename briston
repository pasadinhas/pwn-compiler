				}
         	}
   			case 6://caseeeeee6
			{
				SetPlayerArmour(playerid, 50.0);
				SetPlayerHealth(playerid, 100);
				SendClientMessage(playerid, COLOR_WHITE, "* Ai luat Health+Armour Din vestiar.");
		   		TogglePlayerControllable(playerid,1);
