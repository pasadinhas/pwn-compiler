    new helpeddialog[1900];
	format(strings,sizeof(strings),"{FFFFFF}=========================================================================================================================\n");
	strcat(helpeddialog,strings);
	format(strings,sizeof(strings),"\t\t\t\t\t{004080}*** ACCOUNT *** /forum /rules /login /stats /buylevel\n");
